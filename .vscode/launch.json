    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/html/cards/teste.js"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Run Current File",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Clean-Dic Puppeter",
            "program": "${workspaceFolder}/post/src/clean-dic.py",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}/post/src"
        }
    ]
}